@page "/ListCabinetOfAppoiments"
@inherits ListCabinetOfAppoimentPatientComponentBase



@if (Isloding == true)
{
    <WaitComponent />
}


@foreach (var item in this.jobs)
{
    <div class="card">
        <h5 class="card-header">Job Information</h5>
        <div class="card-body">
            <h5 class="card-title">@item.nameCabinet</h5>
            <p class="card-text">@item.Adress</p>
            <a href="/ListAppoimentPaientDoctor/@item.CabinetId.Replace("/","-")"  class="btn btn-primary">Go List Of Reservation</a>
        </div>
    </div>
}

